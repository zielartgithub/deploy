{
  "family": "java",
  "containerDefinitions": [
    {
      "name": "@NAME@",
      "image": "@IMAGE@:@TAG@",
      "memory": 128,
      "mountPoints": [
      {
        "containerPath" : "/opt/logs",
        "sourceVolume" : "joinus@NAME@",
        "readOnly" : false
      }
      ],
      "portMappings": [
        {
          "containerPort": 8081,
          "protocol": "tcp",
          "hostPort": @PORT@
        }
      ]    
    }
  ]
}
